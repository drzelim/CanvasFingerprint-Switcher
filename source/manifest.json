{
    "manifest_version": 3,
    "name": "CanvasFingerprint-Switcher",
    "version": "0.2.1",
    "author": "DrZelim",
    "action": {
        "default_popup": "popup.html",
        "default_title": "My Popup"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "scripting",
        "activeTab",
        "tabs",
        "storage",
        "webNavigation"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "64": "icons/64.png",
        "128": "icons/128.png"
    }
}
